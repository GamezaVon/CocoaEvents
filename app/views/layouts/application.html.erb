<!DOCTYPE html>
<html>
  <head>
    <title>CocoaEventos</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="description">
    <meta content="" name="keywords">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>


    <!-- Favicons -->
    <%= favicon_link_tag('favicon.png') %>
<!--    <link href="images/favicon.png" rel="icon">-->
    <link href="images/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <%= stylesheet_link_tag('https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i') %>

    <!-- Vendor CSS Files -->
    <%= stylesheet_link_tag('aos.css') %>
    <%= stylesheet_link_tag('bootstrap-icons.css') %>
    <%= stylesheet_link_tag('boxicons.css') %>
    <%= stylesheet_link_tag('swiper-bundle.min.css') %>
    <%= stylesheet_link_tag('glightbox.min.css') %>
    <%= stylesheet_link_tag('bootstrap.css') %>
    <%= stylesheet_link_tag('remixicon.css') %>

    <!-- Template Main CSS File -->
    <%= stylesheet_link_tag('style.css') %>
  </head>

  <body>
    <%= yield %>
  </body>
  <!-- Vendor JS Files -->
  <%= javascript_include_tag("aos.js") %>
  <%= javascript_include_tag("bootstrap.bundle.min.js") %>
  <%= javascript_include_tag("glightbox.min.js") %>
  <%= javascript_include_tag("isotope.pkgd.min.js") %>
  <%= javascript_include_tag("validate.js") %>
  <%= javascript_include_tag("purecounter.js") %>
  <%= javascript_include_tag("swiper-bundle.min.js") %>
  <%= javascript_pack_tag 'main' %>
</html>
