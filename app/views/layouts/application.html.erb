<!DOCTYPE html>
<html>
  <head>
    <title>CocoaEventos</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="description">
    <meta content="" name="keywords">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'main' %>

    <!-- Favicons -->
    <link href="images/favicon.png" rel="icon">
    <link href="images/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="vendor/assets/aos/aos.css" rel="stylesheet">
    <link href="vendor/assets/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="vendor/assets/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="vendor/assets/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="vendor/assets/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="vendor/assets/remixicon/remixicon.css" rel="stylesheet">
    <link href="vendor/assets/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="style.css" rel="stylesheet">
  </head>

  <body>
    <%= yield %>
  </body>
  <!-- Vendor JS Files -->
  <%= javascript_include_tag("aos.js") %>
  <%= javascript_include_tag("bootstrap.bundle.min.js") %>
  <%= javascript_include_tag("glightbox.min.js") %>
  <%= javascript_include_tag("isotope.pkgd.min.js") %>
  <%= javascript_include_tag("validate.js") %>
  <%= javascript_include_tag("purecounter.js") %>
  <%= javascript_include_tag("swiper-bundle.min.js") %>

  <!-- Template Main JS File -->
  <script src="assets/js/main.js"></script>
</html>
